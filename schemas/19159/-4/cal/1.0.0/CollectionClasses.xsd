    <element name="Sequence_Real_" type="cal:Sequence_Real__Type" substitutionGroup="gco:AbstractObject">
        <annotation><documentation>Collection containing multiple gco:Real values.</documentation></annotation>
    </element>
    
    <complexType name="Sequence_Real__Type">
        <complexContent>
            <extension base="gco:AbstractObject_Type">
                <sequence maxOccurs="unbounded">
                    <element name="Real" type="gco:Real_PropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="Sequence_Real__PropertyType">
        <sequence minOccurs="0">
            <element ref="cal:Sequence_Real_"/>
        </sequence>
        <attributeGroup ref="gco:ObjectReference"/>
        <attribute ref="gco:nilReason"/>
    </complexType>
    
    <element name="Sequence_Angle_" type="cal:Sequence_Angle__Type" substitutionGroup="gco:AbstractObject">
        <annotation><documentation>Collection containing multiple gco:Angle values.</documentation></annotation>
    </element>
    <complexType name="Sequence_Angle__Type">
        <complexContent>
            <extension base="gco:AbstractObject_Type">
                <sequence maxOccurs="unbounded">
                    <element name="Angle" type="gco:Angle_PropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="Sequence_Angle__PropertyType">
        <sequence minOccurs="0">
            <element ref="cal:Sequence_Angle_"/>
        </sequence>
        <attributeGroup ref="gco:ObjectReference"/>
        <attribute ref="gco:nilReason"/>
    </complexType>
    
    <element name="Sequence_DateTime_" type="cal:Sequence_DateTime__Type" substitutionGroup="gco:AbstractObject">
        <annotation><documentation>Collection containing multiple gco:DateTime values.</documentation></annotation>
    </element>
    <complexType name="Sequence_DateTime__Type">
        <complexContent>
            <extension base="gco:AbstractObject_Type">
                <sequence maxOccurs="unbounded">
                    <element name="DateTime" type="gco:DateTime_PropertyType"/>
                </sequence>
